{
    "name": "mindtwo/valet-drivers",
    "description": "Ship custom valet drivers with a command to automatically install them.",
    "type": "library",
    "prefer-stable": true,
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mindtwo\\ValetDrivers\\": "src/"
        }
    },
    "require": {
        "laravel/valet": "^2.8"
    },
    "scripts": {
        "post-update-cmd": "@setup",
        "post-install-cmd": "@setup"
    },
    "bin": ["bin/mindtwo-valet-drivers"]
}